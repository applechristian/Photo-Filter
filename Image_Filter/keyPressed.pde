void keyPressed(){ //applies its respective filters when keys 1-7 are pressed
  if(key == '1'){
    filter1();
  }
  if(key == '2'){
    filter2();
  }
  if(key == '3'){
    filter3();
  }
  if(key == '4'){
    filter4();
  }
  if(key == '5'){
    filter5();
  }
  if(key == '6'){
    filter6();
  }
  if(key == '7'){
    noFilter();
  }
}
